{
  "name": "avsb",
  "version": "0.1.0",
  "description": "a very simple build",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg-a06/avsb.git"
  },
  "keywords": [],
  "author": "xg-a06 <xg.jser@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xg-a06/avsb/issues"
  },
  "homepage": "https://github.com/xg-a06/avsb#readme",
  "lint-staged": {
    "packages/**/src/*.{ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "pnpm --filter=@avsb/* run clean && rimraf ./node_modules && pnpm i",
    "preinstall": "npx only-allow pnpm",
    "dev:serve": "pnpm run --filter @avsb/serve start",
    "dev:build": "pnpm run --filter @avsb/build start",
    "dev:utils": "pnpm run --filter @avsb/utils start",
    "dev:cli": "pnpm run --filter @avsb/utils start",
    "dev": "run-p dev:*",
    "build": "pnpm --filter=@avsb/* run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0"
  }
}
